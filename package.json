  "scripts": {
    "dev": "env-cmd -f .env.local node scripts/start.js",
    "start": "env-cmd -f .env.staging node scripts/start.js",
    "build": "env-cmd -f .env.production node scripts/build.js",
    "test": "env-cmd -f .env.testing node scripts/test.js",
    "test-husky": "env-cmd -f .env.testing node scripts/test.js --watchAll=false",
    "test-ci": "env-cmd -f .env.testing node scripts/test.js --watchAll=false",
    "test-coverage": "env-cmd -f .env.testing node scripts/test.js --watchAll=false --coverage",
    "test-codecov": "env-cmd -f .env.testing jest --ci --coverage && codecov --disable=gcov",
    "styleguidist": "node ./node_modules/react-styleguidist/lib/bin/styleguidist.js server",
    "styleguidist:build": "node ./node_modules/react-styleguidist/lib/bin/styleguidist.js build",
    "storybook": "node  ./node_modules/@storybook/react/bin/index.js",
    "storybook:build": "node  ./node_modules/@storybook/react/bin/build.js",
    "prettier": "prettier --write .",
    "lint": "eslint . --fix",
    "codeclimate": "codeclimate analyze ",
    "gh-pages": "gh-pages -d build"
  },
  "homepage": "https://escolalms.github.io/Front/",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "roots": [
      "<rootDir>/src/js"
    ],
    "collectCoverageFrom": [
      "src/js/**/*.{js,jsx,ts,tsx}",
      "!src/js/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/js/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/js/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/js/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "devDependencies": {
    "@storybook/react": "^6.1.4",
    "@types/markdown-it": "^12.0.1",
    "@types/react-slick": "^0.23.4",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "react-docgen-typescript": "^1.20.5",
    "react-styleguidist": "^11.1.3"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test-husky",
      "post-merge": "sh shooks/post-merge.sh",
      "post-rewrite": "sh shooks/post-merge.sh",
      "pre-commit": "eslint . --fix"
    }
  },
  "lint-staged": {
    "*.{js,md,css,html}": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ],
    "yarn.lock": [
      "git rm --cached"
    ]
  }
}
